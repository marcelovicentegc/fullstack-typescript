{
  "name": "butter",
  "version": "1.0.0",
  "main": "index.tsx",
  "author": "Marcelo Cardoso",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js && tsc --p tsconfig.server.json",
    "client": "webpack-dev-server --progress",
    "client-wait": "wait-on tcp:8080 && yarn run client",
    "types": "gql-gen --config codegen.yml",
    "server": "nodemon",
    "start": "cross-env NODE_ENV=development TCP=8080 concurrently \"yarn run server\" \"yarn run client-wait\"",
    "test-watch": "jest --watch",
    "test": "jest",
    "test-wait": "wait-on tcp:8080 && yarn test",
    "test-alone": "cross-env NODE_ENV=development TCP=8080 concurrently --kill-others --success first \"yarn run server\" \"yarn run test-wait\"",
    "upgd": "yarn upgrade-interactive",
    "launch": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/three": "^0.103.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "react-hot-loader": "^4.7.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "react-three": "^0.9.7",
    "three": "^0.104.0",
    "tslint": "^5.14.0"
  }
