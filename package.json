  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js",
    "client": "webpack-dev-server --progress",
    "client-wait": "wait-on tcp:8080 && yarn run client",
    "types": "gql-gen --config codegen.yml",
    "server": "nodemon",
    "start": "concurrently \"yarn run server\" \"yarn run client-wait\"",
    "test-watch": "jest --watch",
    "test": "jest",
    "test-wait": "wait-on tcp:8080 && yarn test",
    "test-alone": "concurrently --kill-others --success first \"yarn run server\" \"yarn run test-wait\"",
    "upgd": "yarn upgrade-interactive"
  },
